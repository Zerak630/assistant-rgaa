{
  "name": "@empreinte-digitale/assistant-rgaa",
  "version": "1.3.0",
  "description": "Assistant RGAA",
  "main": "js/index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "start": "npm run icons && NODE_ENV=development rspack build --watch",
    "icons": "touch ./dist/icons.svg && spritesh --input ./icons --output ./dist/icons.svg",
    "build": "npm run icons && rspack build",
    "build-extension": "npm run build && web-ext build --artifacts-dir artifacts/ --ignore-files ./bin ./css ./doc ./fixtures ./src ./test ./site ./package.json ./webpack.config.*",
    "sign-extension": "npm run build && web-ext sign --artifacts-dir artifacts/ --ignore-files ./bin ./css ./doc ./fixtures ./src ./test ./site ./package.json ./webpack.config.*",
    "lint:format": "prettier --check ./src ./css ./data",
    "lint:js": "eslint ./src",
    "lint:css": "stylelint \"./css/**/*.css\"",
    "lint": "npm run lint:format && npm run lint:js && npm run lint:css",
    "format": "prettier --write ./src ./css ./data",
    "test": "jest",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@jest/globals": "^29.7.0",
    "@rspack/cli": "^0.6.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/color": "^3.0.6",
    "@types/preact-custom-element": "^4.0.4",
    "@types/react-dom": "^18.2.25",
    "@types/webextension-polyfill": "^0.10.7",
    "cheerio": "^0.22.0",
    "commander": "^12.0.0",
    "css-loader": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.0",
    "prettier": "^3.2.5",
    "prettier-loader": "^3.3.0",
    "react-addons-test-utils": "^15.6.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "remark": "^14.0.3",
    "remark-html": "^15.0.2",
    "remark-loader": "^6.0.0",
    "request": "^2.74.0",
    "spritesh": "^1.2.1",
    "stylelint": "^16.3.1",
    "stylelint-config-standard": "^36.0.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "web-ext": "^7.11.0",
    "webextension-polyfill": "^0.11.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.3",
    "@rspack/core": "^0.6.2",
    "classnames": "^2.5.1",
    "color": "^4.2.3",
    "debounce": "^2.0.0",
    "deepmerge": "^4.3.1",
    "hoist-non-react-statics": "^3.3.2",
    "papaparse": "^5.4.1",
    "preact": "^10.20.2",
    "preact-custom-element": "^4.3.0",
    "react-addons-update": "^15.6.3",
    "react-intl": "^6.6.5",
    "react-redux": "^9.1.1",
    "react-tabs": "^6.0.2",
    "redux": "^5.0.1",
    "typescript-eslint": "^7.7.0",
    "wouter": "^3.1.2"
  },
  "volta": {
    "node": "20.0.0"
  }
}
