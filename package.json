{
  "name": "@boscop/assistant-rgaa",
  "version": "1.3.0",
  "description": "Assistant RGAA",
  "type": "module",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "start": "NODE_ENV=development rspack build --watch",
    "build": "rspack build",
    "build-extension": "npm run build && web-ext build --artifacts-dir artifacts/ --ignore-files ./bin ./css ./doc ./e2e ./fixtures ./src ./test ./site ./package.json ./rspack.config.mjs",
    "sign-extension": "npm run build && web-ext sign --artifacts-dir artifacts/ --ignore-files ./bin ./css ./doc ./e2e ./fixtures ./src ./test ./site ./package.json ./rspack.config.mjs",
    "lint:format": "prettier --check .",
    "lint:js": "eslint ./src",
    "lint:css": "stylelint \"./css/**/*.css\"",
    "lint:extension": "web-ext lint --ignore-files ./bin ./css ./doc ./e2e ./fixtures ./src ./test ./site ./package.json ./rspack.config.mjs",
    "lint": "npm run lint:format && npm run lint:js && npm run lint:css && npm run lint:extension",
    "format": "prettier --write .",
    "test:unit": "jest",
    "test:e2e": "playwright test",
    "test": "npm run test:unit && npm run test:e2e",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.51.0",
    "@rspack/cli": "^0.6.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/color": "^3.0.6",
    "@types/node": "^22.13.9",
    "@types/preact-custom-element": "^4.0.4",
    "@types/react-dom": "^18.2.25",
    "@types/webextension-polyfill": "^0.10.7",
    "cheerio": "^0.22.0",
    "colorjs.io": "^0.5.2",
    "commander": "^12.0.0",
    "css-loader": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.5.3",
    "request": "^2.74.0",
    "stylelint": "^16.3.1",
    "stylelint-config-standard": "^36.0.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "web-ext": "^8.4.0",
    "webextension-polyfill": "^0.11.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.3",
    "@rspack/core": "^0.6.2",
    "classnames": "^2.5.1",
    "debounce": "^2.0.0",
    "deepmerge": "^4.3.1",
    "lucide-react": "^0.372.0",
    "preact": "^10.20.2",
    "preact-custom-element": "^4.3.0",
    "react-addons-update": "^15.6.3",
    "react-intl": "^6.6.5",
    "react-redux": "^9.1.1",
    "redux": "^5.0.1"
  },
  "volta": {
    "node": "20.0.0"
  }
}
